{
  "name": "ng-alain-pro",
  "version": "0.0.0",
  "description": "Ng-alain business theme, ng-zorro-antd admin panel front-end framework",
  "author": "cipchk",
  "homepage": "https://e.ng-alain.com/theme/pro",
  "scripts": {
    "ng": "ng",
    "start": "npm run color-less && sudo ng serve -o --disable-host-check --proxy-config proxy.conf.json --port 80",
    "hmr": "npm run color-less && ng serve -c=hmr",
    "build": "npm run color-less && ng build --prod --aot --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "node scripts/color-less.js"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@antv/data-set": "^0.8.8",
    "@antv/g2": "^3.0.11",
    "@antv/g2-plugin-slider": "^2.0.2",
    "@delon/abc": "^2.0.0",
    "@delon/acl": "^2.0.0",
    "@delon/auth": "^2.0.0",
    "@delon/cache": "^2.0.0",
    "@delon/chart": "^2.0.0",
    "@delon/form": "^2.0.0",
    "@delon/mock": "^2.0.0",
    "@delon/theme": "^2.0.0",
    "@delon/util": "^2.0.0",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-charts": "^10.0.0",
    "@types/jquery": "^3.2.12",
    "@types/lodash": "^4.14.62",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.2.34",
    "abp-ng2-module": "^3.2.3",
    "abp-web-resources": "^3.8.4",
    "ajv": "^6.4.0",
    "angular-baidu-maps": "^3.0.0",
    "angular-sortablejs": "^2.6.0",
    "core-js": "^2.5.4",
    "file-saver": "^1.3.8",
    "fullcalendar": "^4.0.0-alpha.2",
    "jquery": "^3.1.1",
    "lodash": "^4.17.11",
    "masonry-layout": "^4.2.2",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.13",
    "ng-zorro-antd": "^1.9.0-rc.0",
    "ngx-countdown": "^3.0.0",
    "ngx-image-gallery": "^1.3.1",
    "ngx-trend": "^3.4.1",
    "perfect-scrollbar": "^1.4.0",
    "qrious": "^4.0.2",
    "quill": "^1.3.6",
    "quill-image-resize-module": "^3.0.0",
    "rtl-detect": "^1.0.2",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.3.3",
    "screenfull": "^3.3.1",
    "sortablejs": "^1.7.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.4",
    "@angular/cli": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.3",
    "@types/mockjs": "^1.0.0",
    "@types/node": "~8.9.4",
    "codecov": "^3.1.0",
    "codelyzer": "~4.3.0",
    "editorconfig-tools": "^0.1.1",
    "gh-pages": "^1.1.0",
    "husky": "^1.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-bundle-promise": "^1.0.7",
    "lint-staged": "^7.1.0",
    "mockjs": "^1.0.1-beta3",
    "ng-alain": "^2.0.0",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~3.1.1",
    "webpack-bundle-analyzer": "^2.11.2",
    "xlsx": "^0.12.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ],
    "ignore": [
      "src/assets/*"
    ]
  },
  "private": true
}
